@model IEnumerable<scaffef.Models.tblProject>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OpenedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PctComplete)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.tblContact.LastName)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.tblEndUser.EndUserName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.tblPriority.PriorityName)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.tblStatu.StatusName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OpenedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PctComplete)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.tblContact.LastName)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.tblEndUser.EndUserName)
        </td>
       
     
        <td>
            @Html.DisplayFor(modelItem => item.tblPriority.PriorityName)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.tblStatu.StatusName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProjectID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProjectID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProjectID })
        </td>
    </tr>
}

</table>
